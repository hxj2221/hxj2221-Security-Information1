BA;AACA,UAAA,YAAA,EAAA,KAAA,UAjBA,CAiBA;;AAjBA,SAAA;AAmBA,QAAA,OAAA,CAAA,YAAA,CAAA,MAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AACA,cAAA,GAAA,CAAA,IAAA,IAAA,KAAA,EAAA;AACA,YAAA,KAAA,CAAA,QAAA,CAAA;AACA,cAAA,OAAA,EAAA,GAAA,CAAA,GADA;AAEA,cAAA,IAAA,EAAA,SAFA;AAGA,cAAA,QAAA,EAAA;AAHA,aAAA;;AAKA,YAAA,KAAA,CAAA,OAAA,CAAA,EAAA,CAAA,CAAA;AACA,WAPA,MAQA;AACA,YAAA,KAAA,CAAA,QAAA,CAAA;AACA,cAAA,OAAA,EAAA,GAAA,CAAA,GADA;AAEA,cAAA,IAAA,EAAA,OAFA;AAGA,cAAA,QAAA,EAAA;AAHA,aAAA;AAKA;AAEA,SAlBA;AAmBA;AACA,KAhDA;AAiDA,IAAA,MAjDA,oBAiDA;AACA,WAAA,OAAA,CAAA,EAAA,CAAA,CAAA;AACA,KAnDA;AAqDA;AACA,IAAA,WAtDA,uBAsDA,KAtDA,EAsDA,IAtDA,EAsDA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,EAAA,IAAA;AACA,KAxDA;AAyDA;AACA,IAAA,WA1DA,6BA0DA;AAAA,UAAA,QAAA,QAAA,QAAA;;AACA,UAAA,QAAA,IAAA,CAAA,EAAA;AACA,eAAA,+BAAA;AACA,OAFA,MAEA;AACA,eAAA,EAAA;AACA;AACA;AAhEA,GAjEA;AAmIA,EAAA,OAnIA,qBAmIA;AAAA;;AACA,QAAA,KAAA,GAAA,cAAA,CAAA,OAAA,CAAA,OAAA,CAAA;AACA,IAAA,OAAA,CAAA,GAAA,CAAA,KAAA;;AACA,QAAA,KAAA,KAAA,IAAA,IAAA,KAAA,KAAA,EAAA,EAAA;AACA,MAAA,OAAA,CACA,MADA,GAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AACA,YAAA,GAAA,CAAA,IAAA,IAAA,KAAA,EAAA;AACA,UAAA,MAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA,CAAA,YAAA,CADA,CACA;;AACA,UAAA,MAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA,CAAA,aAAA,CAFA,CAEA;;AACA,UAAA,MAAA,CAAA,WAAA,GAAA,GAAA,CAAA,IAAA,CAAA,UAAA,CAHA,CAGA;;AACA,UAAA,MAAA,CAAA,OAAA,GAAA,GAAA,CAAA,IAAA,CAAA,SAAA,CAJA,CAIA;;AACA,UAAA,MAAA,CAAA,YAAA,GAAA,GAAA,CAAA,IAAA,CAAA,kBAAA,CALA,CAKA;;AACA,UAAA,MAAA,CAAA,MAAA,GAAA,GAAA,CAAA,IAAA,CAAA,UAAA,CANA,CAMA;AACA,SAPA,MAOA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA;AACA,YAAA,OAAA,EAAA,GAAA,CAAA,GADA;AAEA,YAAA,IAAA,EAAA,OAFA;AAGA,YAAA,QAAA,EAAA;AAHA,WAAA;AAKA;AACA,OAlBA,WAmBA,UAAA,GAAA,EAAA,CAAA,CAnBA;AAoBA;AACA;AA5JA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"addcom\">\r\n      <div class=\"addcom-top\">\r\n        <h4>基本信息</h4>\r\n      </div>\r\n      <div class=\"addcom-form-basic\">\r\n        <el-row type=\"flex\" class=\"row-bg\" justify=\"space-around\">\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple\">\r\n              <span> 业务编号 </span>\r\n              <el-input v-model=\"comnumber\" disabled></el-input>\r\n            </div>\r\n          </el-col>\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple-light\">\r\n              <span> 投诉日期<span>*</span> </span> <br />\r\n              <el-date-picker\r\n                v-model=\"comdata\"\r\n                type=\"date\"\r\n                placeholder=\"选择日期\"\r\n                :picker-options=\"pickerOptions\"\r\n              ></el-date-picker></div\r\n          ></el-col>\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple-light\">\r\n              <span> 事发日期<span>*</span> </span> <br />\r\n              <el-date-picker v-model=\"incidentdata\" type=\"date\" placeholder=\"选择日期\"   :picker-options=\"pickerOption\">\r\n              </el-date-picker></div\r\n          ></el-col>\r\n        </el-row>\r\n        <el-row type=\"flex\" class=\"row-bg\" justify=\"space-around\">\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple\">\r\n              <span> 投诉人姓名 <span>*</span></span>\r\n              <el-input v-model=\"comname\" placeholder=\"请输入投诉人姓名\"></el-input>\r\n            </div>\r\n          </el-col>\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple-light\">\r\n              <span> 性别<span>*</span> </span> <br />\r\n              <el-select v-model=\"comgender\" placeholder=\"请选择\">\r\n                <el-option\r\n                  v-for=\"item in comgenders\"\r\n                  :key=\"item.value\"\r\n                  :label=\"item.lable\"\r\n                  :value=\"item.value\"\r\n                >\r\n                </el-option>\r\n              </el-select></div\r\n          ></el-col>\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple-light\">\r\n              <span> 年龄<span>*</span></span> <br />\r\n              <el-input\r\n                placeholder=\"请输入\"\r\n                v-model=\"comagenumber\"\r\n                class=\"input-with-select\"\r\n                type='number'\r\n               oninput=\"value=value.replace(/[^\\d]/g,'')\"\r\n               onkeypress=\"return( /[\\d]/.test(String.fromCharCode(event.keyCode) ) )\"\r\n              >\r\n                <template slot=\"append\">\r\n                  <el-select v-model=\"comage\" slot=\"prepend\" style=\"width: 80px\">\r\n                    <el-option label=\"岁\" value=\"1\"></el-option>\r\n                    <el-option label=\"月\" value=\"2\"></el-option>\r\n                    <el-option label=\"日\" value=\"3\"></el-option> </el-select\r\n                ></template>\r\n              </el-input></div\r\n          ></el-col>\r\n        </el-row>\r\n        <el-row type=\"flex\" class=\"row-bg\" justify=\"space-around\">\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple\">\r\n              <span> 手机号码 <span>*</span></span>\r\n              <el-input v-model=\"comphone\" placeholder=\"请输入投诉人手机号码\" type=\"input\" maxlength=\"11\" max=\"11\"     \r\n               oninput=\"value=value.replace(/[^\\d]/g,'')\"\r\n               onkeypress=\"return( /[\\d]/.test(String.fromCharCode(event.keyCode) ) )\"  ></el-input>\r\n            </div>\r\n          </el-col>\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple-light\">\r\n              <span> 投诉方式<span>*</span> </span> <br />\r\n              <el-select v-model=\"commode\" placeholder=\"请选择\">\r\n                <el-option\r\n                  v-for=\"item in commodes\"\r\n                  :key=\"item.id\"\r\n                  :label=\"item.title\"\r\n                  :value=\"item.id\"\r\n                >\r\n                </el-option>\r\n              </el-select></div\r\n          ></el-col>\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple-light\">\r\n              <span> 投诉科室<span>*</span> </span> <br />\r\n              <el-cascader\r\n                ref=\"cascader\"\r\n                :options=\"comdes\"\r\n                :props=\"{\r\n                  value: 'id',\r\n                  label: 'title',\r\n                  children: '_child',\r\n                  multiple: 'true',\r\n                }\"\r\n                :show-all-levels=\"false\"\r\n                v-model=\"comde\"\r\n                clearable\r\n                @change=\"getCascaderObj\"\r\n              ></el-cascader></div\r\n          ></el-col>\r\n        </el-row>\r\n        <el-row type=\"flex\" class=\"row-bg\" justify=\"space-around\">\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple\">\r\n              <span> 投诉人与患者关系 <span>*</span></span><br />\r\n              <el-select v-model=\"relation\" placeholder=\"请选择\">\r\n                <el-option\r\n                  v-for=\"item in relationlist\"\r\n                  :key=\"item.id\"\r\n                  :label=\"item.title\"\r\n                  :value=\"item.id\"\r\n                >\r\n                </el-option>\r\n              </el-select>\r\n            </div>\r\n          </el-col>\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple-light\">\r\n              <span> 答复协商时间</span> <br />\r\n              <el-input placeholder=\"请输入\" v-model=\"consulttime\" style=\"width: 60%\" type=\"input\" maxlength=\"3\" max=\"3\"     \r\n               oninput=\"value=value.replace(/[^\\d]/g,'')\"\r\n               onkeypress=\"return( /[\\d]/.test(String.fromCharCode(event.keyCode) ) )\" >\r\n                <template slot=\"append\">天</template>\r\n              </el-input>\r\n            </div></el-col\r\n          >\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple-light\">\r\n              <span> 投诉类别(多选)</span> <br />\r\n              <el-select v-model=\"comtype\" multiple placeholder=\"请选择\">\r\n                <el-option\r\n                  v-for=\"item in comtypelist\"\r\n                  :key=\"item.id\"\r\n                  :label=\"item.title\"\r\n                  :value=\"item.id\"\r\n                >\r\n                </el-option>\r\n              </el-select></div\r\n          ></el-col>\r\n        </el-row>\r\n        <el-row type=\"flex\" class=\"row-bg\">\r\n          <el-col :span=\"6\" :push=\"1\"\r\n            ><div class=\"grid-content bg-purple\">\r\n              <span> 事件性质 </span><br />\r\n              <el-select v-model=\"nature\" placeholder=\"请选择\">\r\n                <el-option\r\n                  v-for=\"item in natures\"\r\n                  :key=\"item.id\"\r\n                  :label=\"item.title\"\r\n                  :value=\"item.title\"\r\n                >\r\n                </el-option>\r\n              </el-select>\r\n            </div>\r\n          </el-col>\r\n          <el-col :span=\"12\" :push=\"3\"\r\n            ><div class=\"grid-content bg-purple-light\">\r\n              <span> 联系地址<span>*</span> </span> <br />\r\n              <el-input v-model=\"address\" placeholder=\"联系地址\"></el-input></div\r\n          ></el-col>\r\n        </el-row>\r\n        <el-row type=\"flex\" class=\"row-bg\">\r\n          <el-col :span=\"20\" :push=\"1\"\r\n            ><div class=\"grid-content bg-purple\">\r\n              <span> 投诉原因</span\r\n              ><el-input\r\n                type=\"textarea\"\r\n                v-model=\"reason\"\r\n                placeholder=\"请输入投诉原因\"\r\n              ></el-input>\r\n            </div>\r\n          </el-col>\r\n        </el-row>\r\n      </div>\r\n      <!-- 经办人 -->\r\n      <div class=\"Addcom-agent\">\r\n        <h4>经办人信息</h4>\r\n        <div>\r\n          <el-row type=\"flex\" class=\"row-bg\" justify=\"space-around\">\r\n            <el-col :span=\"6\" :pull=\"2\"\r\n              ><div class=\"grid-content bg-purple\">\r\n                <span> 经办人姓名<span>*</span> </span>\r\n                <el-input v-model=\"agentname\" placeholder=\"请输入姓名\"  type=\"input\" maxlength=\"8\"></el-input>\r\n              </div>\r\n            </el-col>\r\n            <el-col :span=\"6\" :pull=\"6\"\r\n              ><div class=\"grid-content bg-purple-light\">\r\n                <span>经办人手机<span>*</span> </span> <br />\r\n                <el-input\r\n                  v-model=\"agentphone\"\r\n                  placeholder=\"请输入手机号码\"\r\n                  type=\"input\" maxlength=\"11\" max=\"11\"     \r\n               oninput=\"value=value.replace(/[^\\d]/g,'')\"\r\n               onkeypress=\"return( /[\\d]/.test(String.fromCharCode(event.keyCode) ) )\"\r\n                ></el-input></div\r\n            ></el-col>\r\n          </el-row>\r\n        </div>\r\n      </div>\r\n      <!-- 患者信息 -->\r\n      <div class=\"addcom-relevance\" v-show=\"false\">\r\n        <h4>关联患者信息(调用HIS)</h4>\r\n        <el-table\r\n          :data=\"data\"\r\n          style=\"width: 94%; margin-left: 3%; margin-top: 20px; margin-bottom: 20px\"\r\n          :header-cell-style=\"getRowClass\"\r\n        >\r\n          <el-table-column type=\"index\" width=\"50\" label=\"序号\"> </el-table-column>\r\n          <el-table-column prop=\"name\" label=\"姓名\"> </el-table-column>\r\n          <el-table-column prop=\"name\" label=\"性别\"> </el-table-column>\r\n          <el-table-column prop=\"name\" label=\"出生日期\"> </el-table-column>\r\n          <el-table-column prop=\"name\" label=\"联系方式\"> </el-table-column>\r\n          <el-table-column prop=\"name\" label=\"地址\"> </el-table-column>\r\n          <el-table-column prop=\"name\" label=\"科室\"> </el-table-column>\r\n          <el-table-column prop=\"name\" label=\"诊断\"> </el-table-column>\r\n          <el-table-column\r\n            fixed=\"right\"\r\n            label=\"操作\"\r\n            width=\"100%\"\r\n            style=\"border-bottom: none\"\r\n          >\r\n            <template slot-scope=\"scope\">\r\n              <el-button\r\n                type=\"text\"\r\n                size=\"small\"\r\n                @click=\"handleClick(scope.row)\"\r\n                style=\"border-bottom: none\"\r\n                >取消关联</el-button\r\n              >\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n        <div align=\"center\">\r\n          <el-button type=\"primary\" icon=\"el-icon-circle-plus\" class=\"addrelevance\"\r\n            >添加患者信息</el-button\r\n          >\r\n        </div>\r\n      </div>\r\n      <!-- 弹窗 -->\r\n      <div class=\"requirement\">\r\n        <slot name=\"dialog\"> </slot>\r\n      </div>\r\n      <!-- <slot name=\"conserve\"> -->\r\n      <Conserve>\r\n        <el-button\r\n          type=\"primary\"\r\n          icon=\"iconfont el-icon-hospital-passwordbaocun\"\r\n          class=\"keep\"\r\n          slot=\"keep\"\r\n          @click=\"keepform()\"\r\n          >提交</el-button\r\n        >\r\n        <el-button\r\n          type=\"primary\"\r\n          icon=\"iconfont el-icon-hospital-passwordai207\"\r\n          class=\"return\"\r\n          style=\"border: 1px solid #949aef\"\r\n          slot=\"back\"\r\n          @click=\"backss()\"\r\n          >返回</el-button\r\n        >\r\n      </Conserve>\r\n      <!-- </slot> -->\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport Conserve from \"../components/conserve\";\r\n// 获取列表\r\nimport service from \"@/service/index\";\r\n\r\nexport default {\r\n  components: {\r\n    Conserve,\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      props: { multiple: true },\r\n      agree: \"\", //同意要求\r\n      pickerOptions: {\r\n        disabledDate(time) {\r\n          return time.getTime() > Date.now();\r\n        },\r\n      },\r\n        pickerOption: {\r\n        disabledDate(time) {\r\n          return time.getTime() >  Date.now();\r\n        },\r\n      },\r\n      data: [\r\n        {\r\n          name: 1,\r\n        },\r\n      ],\r\n      agentname: \"\", //经办人姓名\r\n      agentphone: \"\", //经办人手机号\r\n      comnumber: \"\", //业务编号\r\n      comdata: \"\", //投诉日期\r\n      incidentdata: \"\", //事发日期\r\n      comname: \"\", //投诉人姓名\r\n      comgender: \"\", //投诉人性别\r\n      comage: \"岁\", //投诉人年龄\r\n      comagenumber: \"\", //投诉人年龄\r\n      comphone: \"\", //投诉人电话\r\n      consulttime: \"\", //协商时间\r\n      address: \"\", //联系地址\r\n      reason: \"\", //投诉原因\r\n      comgenders: [\r\n        //性别\r\n        {\r\n          value: \"0\",\r\n          lable: \"男\",\r\n        },\r\n        {\r\n          value: \"1\",\r\n          lable: \"女\",\r\n        },\r\n        {\r\n          value: \"2\",\r\n          lable: \"未知\",\r\n        },\r\n      ],\r\n      commodes: [], // 投诉方式列表\r\n      commode: \"\", //投诉方式\r\n      comdes: [], //投诉科室列表\r\n      comde: \"\", //投诉科室\r\n      relationlist: [], //投诉人与患者的关系\r\n      relation: \"\", //关系列表\r\n      comtypelist: [], //投诉类别列表\r\n      comtype: \"\", //投诉类别\r\n      natures: [], //事件性质列表\r\n      nature: \"\", //事件性质\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    getCascaderObj() {\r\n    },\r\n    //保存提交事件\r\n    keepform() {\r\n      if (this.comde !== \"\" || this.comde !== null) {\r\n        let comde = this.comde.map((x) => {\r\n          return x[0];\r\n        });\r\n      let params = {\r\n        event_number: this.comnumber, //业务编号\r\n        event_type: this.comtype, //投诉类型\r\n        cause: this.reason, //投诉原因\r\n        occur_time: new Date(this.incidentdata).getTime(), //事发时间\r\n        department_id: comde, //投诉科室\r\n        create_time: new Date(this.comdata).getTime(), //投诉日期\r\n        ComplaintType: this.commode, //投诉方式\r\n        complaint_name: this.comname, //投诉人姓名\r\n        complaint_phone: this.comphone, //投诉人电话\r\n        sex: this.comgender, //投诉人电话\r\n        specific_age: 1, //投诉人年龄\r\n        age: this.comagenumber, //投诉人年龄\r\n        inpatient_relation: this.relation, //患者关系\r\n        reply_time: this.consulttime, //协商时间\r\n        character: 1, //事件性质this.nature\r\n        handle_name: this.agentname, //经办人姓名\r\n        handle_phone: this.agentphone, //经办人手机号\r\n      };\r\n      service.AddComponent(params).then((res) => {\r\n        console.log(res);\r\n        if(res.code==20010){\r\n           this.$message({\r\n            message: res.msg,\r\n            type: \"success\",\r\n            duration: 1000,\r\n          });\r\n          this.$router.go(0);\r\n        }\r\n        else{\r\n          this.$message({\r\n            message: res.msg,\r\n            type: \"error\",\r\n            duration: 1000,\r\n          });\r\n        }\r\n        \r\n      });\r\n       }\r\n    },\r\n    backss() {\r\n      this.$router.go(0);\r\n    },\r\n\r\n    //表格点击事件\r\n    handleClick(index, rows) {\r\n      console.log(index, rows);\r\n    },\r\n    // 设置表头颜色\r\n    getRowClass({ rowIndex }) {\r\n      if (rowIndex == 0) {\r\n        return \"background:#c2c5f6;color:#000\";\r\n      } else {\r\n        return \"\";\r\n      }\r\n    },\r\n  },\r\n  created() {\r\n     let token=sessionStorage.getItem('token')\r\n     console.log(token)\r\n    if(token!==null&&token!==''){\r\n    service\r\n      .AddCom()\r\n      .then((res) => {\r\n        console.log(res);\r\n        if (res.code == 20010) {\r\n          this.comnumber = res.data.event_number; // 事件编号\r\n          this.commodes = res.data.ComplaintType; // 事件编号\r\n          this.comtypelist = res.data.event_type; // 投诉方式\r\n          this.natures = res.data.character; // 事件性质\r\n          this.relationlist = res.data.inpatient_relation; // 关系\r\n          this.comdes = res.data.department; //投诉科室\r\n        } else {\r\n          this.$message({\r\n            message: res.msg,\r\n            type: \"error\",\r\n            duration: 1000,\r\n          });\r\n        }\r\n      })\r\n      .catch((err) => {});\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n@import \"../css/addcom.css\";\r\n</style>\r\n"],"sourceRoot":"src/views/H-Complaints/components"}]}
a.selected.push(item);\r\n          });\r\n          console.log(data.id);\r\n        });\r\n      } else {\r\n        this.datas.forEach(function (data) {\r\n          data.selected = [];\r\n        });\r\n      }\r\n    },\r\n    isAllChecked() {\r\n      return this.datas.every(function (data) {\r\n        return data.selected.length === data.name.length;\r\n      });\r\n    },\r\n    changesonc(e) {\r\n      console.log(e);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n</style>\r\n"],"sourceRoot":"src/views/H-Edit/component"}]}
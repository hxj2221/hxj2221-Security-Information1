AAYA;AACA,QAAA,MAAA,EAAA,KAAA,QAbA;AAaA;AACA,QAAA,UAAA,EAAA,KAAA,WAdA;AAcA;AACA,QAAA,aAAA,EAAA,KAAA,MAfA;AAeA;AACA,QAAA,OAAA,EAAA,KAAA,SAhBA;AAgBA;AACA,QAAA,WAAA,EAAA,KAAA,YAjBA;AAiBA;AACA,QAAA,QAAA,EAAA,KAAA,UAlBA;AAkBA;AACA,QAAA,IAAA,EAAA,CAnBA,CAmBA;;AAnBA,OAAA;AAsBA,UAAA,GAAA,GAAA,CAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,MAAA;AACA,MAAA,OAAA,CAAA,YAAA,CAAA,EAAA,CAAA,SAAA,CAAA,MAAA,CAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA,CACA;AACA,OAFA;AAGA,KApDA;AAqDA,IAAA,MArDA,oBAqDA;AACA,WAAA,OAAA,CAAA,EAAA,CAAA,CAAA;AACA,KAvDA;AAyDA;AACA,IAAA,WA1DA,uBA0DA,KA1DA,EA0DA,IA1DA,EA0DA,CACA;AACA,KA5DA;AA6DA;AACA,IAAA,WA9DA,6BA8DA;AAAA,UAAA,QAAA,QAAA,QAAA;;AACA,UAAA,QAAA,IAAA,CAAA,EAAA;AACA,eAAA,+BAAA;AACA,OAFA,MAEA;AACA,eAAA,EAAA;AACA;AACA;AApEA,GA1DA;AAgIA,EAAA,OAhIA,qBAgIA;AAAA;;AACA,IAAA,OAAA,CACA,UADA,CACA,CADA,EAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA;AACA,MAAA,KAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA,CAFA,CAEA;AACA,KALA,WAMA,UAAA,GAAA,EAAA,CAAA,CANA;AAOA,IAAA,OAAA,CAAA,aAAA,GAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA;AACA,MAAA,KAAA,CAAA,MAAA,GAAA,GAAA,CAAA,IAAA,CAFA,CAEA;AACA,KAHA;AAIA,IAAA,OAAA,CAAA,MAAA,GAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA;AACA,MAAA,KAAA,CAAA,SAAA,GAAA,GAAA,CAAA,MAAA,CAFA,CAEA;AACA,KAHA;AAIA,IAAA,OAAA,CAAA,UAAA,CAAA,CAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA;AACA,MAAA,KAAA,CAAA,YAAA,GAAA,GAAA,CAAA,IAAA,CAFA,CAEA;AACA,KAHA;AAIA,IAAA,OAAA,CAAA,UAAA,CAAA,CAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA;AACA,MAAA,KAAA,CAAA,WAAA,GAAA,GAAA,CAAA,IAAA,CAFA,CAEA;AACA,KAHA;AAIA;AAxJA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"addcom\">\r\n      <div class=\"addcom-top\">\r\n        <h4>基本信息</h4>\r\n      </div>\r\n      <div class=\"addcom-form-basic\">\r\n        <el-row type=\"flex\" class=\"row-bg\" justify=\"space-around\">\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple\">\r\n              <span> 业务编号 </span>\r\n              <el-input v-model=\"comnumber\" disabled></el-input>\r\n            </div>\r\n          </el-col>\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple-light\">\r\n              <span> 投诉日期<span>*</span> </span> <br />\r\n              <el-date-picker\r\n                v-model=\"comdata\"\r\n                type=\"date\"\r\n                placeholder=\"选择日期\"\r\n              ></el-date-picker></div\r\n          ></el-col>\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple-light\">\r\n              <span> 事发日期<span>*</span> </span> <br />\r\n              <el-date-picker\r\n                v-model=\"incidentdata\"\r\n                type=\"date\"\r\n                placeholder=\"选择日期\"\r\n              >\r\n              </el-date-picker></div\r\n          ></el-col>\r\n        </el-row>\r\n        <el-row type=\"flex\" class=\"row-bg\" justify=\"space-around\">\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple\">\r\n              <span> 投诉人姓名 </span>\r\n              <el-input\r\n                v-model=\"comname\"\r\n                placeholder=\"请输入投诉人姓名\"\r\n              ></el-input>\r\n            </div>\r\n          </el-col>\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple-light\">\r\n              <span> 性别<span>*</span> </span> <br />\r\n              <el-select v-model=\"comgender\" placeholder=\"请选择\">\r\n                <el-option\r\n                  v-for=\"item in comgenders\"\r\n                  :key=\"item.value\"\r\n                  :label=\"item.lable\"\r\n                  :value=\"item.value\"\r\n                >\r\n                </el-option>\r\n              </el-select></div\r\n          ></el-col>\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple-light\">\r\n              <span> 年龄</span> <br />\r\n              <el-input\r\n                placeholder=\"请输入\"\r\n                v-model=\"comagenumber\"\r\n                class=\"input-with-select\"\r\n              >\r\n                <template slot=\"append\">\r\n                  <el-select\r\n                    v-model=\"comage\"\r\n                    slot=\"prepend\"\r\n                    style=\"width: 80px\"\r\n                  >\r\n                    <el-option label=\"岁\" value=\"0\"></el-option>\r\n                    <el-option label=\"月\" value=\"1\"></el-option>\r\n                    <el-option label=\"日\" value=\"2\"></el-option> </el-select\r\n                ></template>\r\n              </el-input></div\r\n          ></el-col>\r\n        </el-row>\r\n        <el-row type=\"flex\" class=\"row-bg\" justify=\"space-around\">\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple\">\r\n              <span> 手机号码 </span>\r\n              <el-input\r\n                v-model=\"comphone\"\r\n                placeholder=\"请输入投诉人手机号码\"\r\n              ></el-input>\r\n            </div>\r\n          </el-col>\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple-light\">\r\n              <span> 投诉方式<span>*</span> </span> <br />\r\n              <el-select v-model=\"commode\" placeholder=\"请选择\">\r\n                <el-option\r\n                  v-for=\"item in commodes\"\r\n                  :key=\"item.D_M_ID\"\r\n                  :label=\"item.D_M_Name\"\r\n                  :value=\"item.D_M_ID\"\r\n                >\r\n                </el-option>\r\n              </el-select></div\r\n          ></el-col>\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple-light\">\r\n              <span> 投诉科室<span>*</span> </span> <br />\r\n              <!-- <el-select v-model=\"comde\" placeholder=\"请选择\">\r\n                <el-option\r\n                  v-for=\"item in comdes\"\r\n                  :key=\"item.value\"\r\n                  :label=\"item.label\"\r\n                  :value=\"item.value\"\r\n                >\r\n                </el-option>\r\n              </el-select> -->\r\n              <el-cascader\r\n                ref=\"cascader\"\r\n                :options=\"comdes\"\r\n                :props=\"{\r\n                  value: 'D_ID',\r\n                  label: 'Name',\r\n                  children: 'list',\r\n                  multiple: 'true',\r\n                }\"\r\n                :show-all-levels=\"false\"\r\n                v-model=\"comde\"\r\n                clearable\r\n                @change=\"getCascaderObj\"\r\n              ></el-cascader></div\r\n          ></el-col>\r\n        </el-row>\r\n        <el-row type=\"flex\" class=\"row-bg\" justify=\"space-around\">\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple\">\r\n              <span> 投诉人与患者关系 </span><br />\r\n              <el-select v-model=\"relation\" placeholder=\"请选择\">\r\n                <el-option\r\n                  v-for=\"item in relationlist\"\r\n                  :key=\"item.D_M_ID\"\r\n                  :label=\"item.D_M_Name\"\r\n                  :value=\"item.D_M_ID\"\r\n                >\r\n                </el-option>\r\n              </el-select>\r\n            </div>\r\n          </el-col>\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple-light\">\r\n              <span> 答复协商时间</span> <br />\r\n              <el-input\r\n                placeholder=\"请输入\"\r\n                v-model=\"consulttime\"\r\n                style=\"width: 60%\"\r\n              >\r\n                <template slot=\"append\">天</template>\r\n              </el-input>\r\n            </div></el-col\r\n          >\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple-light\">\r\n              <span> 投诉类别(多选)</span> <br />\r\n              <el-select v-model=\"comtype\" multiple placeholder=\"请选择\">\r\n                <el-option\r\n                  v-for=\"item in comtypelist\"\r\n                  :key=\"item.D_M_ID\"\r\n                  :label=\"item.D_M_Name\"\r\n                  :value=\"item.D_M_ID\"\r\n                >\r\n                </el-option>\r\n              </el-select></div\r\n          ></el-col>\r\n        </el-row>\r\n        <el-row type=\"flex\" class=\"row-bg\">\r\n          <el-col :span=\"6\" :push=\"1\"\r\n            ><div class=\"grid-content bg-purple\">\r\n              <span> 事件性质 </span><br />\r\n              <el-select v-model=\"nature\" placeholder=\"请选择\">\r\n                <!-- <el-option\r\n                  v-for=\"item in natures\"\r\n                  :key=\"item.D_M_ID\"\r\n                  :label=\"item.D_M_Name\"\r\n                  :value=\"item.D_M_ID\"\r\n                >\r\n                </el-option> -->\r\n                <el-option label=\"投诉\" value=\"0\">投诉</el-option>\r\n                <el-option label=\"纠纷\" value=\"1\">纠纷</el-option>\r\n                <el-option label=\"投诉转纠纷\" value=\"2\">投诉转纠纷</el-option>\r\n              </el-select>\r\n            </div>\r\n          </el-col>\r\n          <el-col :span=\"12\" :push=\"3\"\r\n            ><div class=\"grid-content bg-purple-light\">\r\n              <span> 联系地址<span>*</span> </span> <br />\r\n              <el-input\r\n                v-model=\"address\"\r\n                placeholder=\"联系地址\"\r\n              ></el-input></div\r\n          ></el-col>\r\n        </el-row>\r\n        <el-row type=\"flex\" class=\"row-bg\">\r\n          <el-col :span=\"20\" :push=\"1\"\r\n            ><div class=\"grid-content bg-purple\">\r\n              <span> 投诉原因</span\r\n              ><el-input\r\n                type=\"textarea\"\r\n                v-model=\"reason\"\r\n                placeholder=\"请输入投诉原因\"\r\n              ></el-input>\r\n            </div>\r\n          </el-col>\r\n        </el-row>\r\n      </div>\r\n      <!-- 经办人 -->\r\n      <div class=\"Addcom-agent\">\r\n        <h4>经办人信息</h4>\r\n        <div>\r\n          <el-row type=\"flex\" class=\"row-bg\" justify=\"space-around\">\r\n            <el-col :span=\"6\" :pull=\"2\"\r\n              ><div class=\"grid-content bg-purple\">\r\n                <span> 经办人姓名<span>*</span> </span>\r\n                <el-input\r\n                  v-model=\"newagentname\"\r\n                  placeholder=\"请输入姓名\"\r\n                ></el-input>\r\n                <el-input v-model=\"agentname\" disabled v-show=\"false\"></el-input\r\n                ><!--老的名字 -->\r\n              </div>\r\n            </el-col>\r\n            <el-col :span=\"6\" :pull=\"6\"\r\n              ><div class=\"grid-content bg-purple-light\">\r\n                <span>经办人手机<span>*</span> </span> <br />\r\n                <el-input\r\n                  v-model=\"agentphone\"\r\n                  placeholder=\"请输入手机号码\"\r\n                ></el-input></div\r\n            ></el-col>\r\n          </el-row>\r\n        </div>\r\n      </div>\r\n      <!-- 患者信息 -->\r\n      <div class=\"addcom-relevance\" v-show=\"false\">\r\n        <h4>关联患者信息(调用HIS)</h4>\r\n        <el-table\r\n          :data=\"data\"\r\n          style=\"\r\n            width: 94%;\r\n            margin-left: 3%;\r\n            margin-top: 20px;\r\n            margin-bottom: 20px;\r\n          \"\r\n          :header-cell-style=\"getRowClass\"\r\n        >\r\n          <el-table-column type=\"index\" width=\"50\" label=\"序号\">\r\n          </el-table-column>\r\n          <el-table-column prop=\"name\" label=\"姓名\"> </el-table-column>\r\n          <el-table-column prop=\"name\" label=\"性别\"> </el-table-column>\r\n          <el-table-column prop=\"name\" label=\"出生日期\"> </el-table-column>\r\n          <el-table-column prop=\"name\" label=\"联系方式\"> </el-table-column>\r\n          <el-table-column prop=\"name\" label=\"地址\"> </el-table-column>\r\n          <el-table-column prop=\"name\" label=\"科室\"> </el-table-column>\r\n          <el-table-column prop=\"name\" label=\"诊断\"> </el-table-column>\r\n          <el-table-column\r\n            fixed=\"right\"\r\n            label=\"操作\"\r\n            width=\"100%\"\r\n            style=\"border-bottom: none\"\r\n          >\r\n            <template slot-scope=\"scope\">\r\n              <el-button\r\n                type=\"text\"\r\n                size=\"small\"\r\n                @click=\"handleClick(scope.row)\"\r\n                style=\"border-bottom: none\"\r\n                >取消关联</el-button\r\n              >\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n        <div align=\"center\">\r\n          <el-button\r\n            type=\"primary\"\r\n            icon=\"el-icon-circle-plus\"\r\n            class=\"addrelevance\"\r\n            >添加患者信息</el-button\r\n          >\r\n        </div>\r\n      </div>\r\n      <!-- 弹窗 -->\r\n      <div class=\"requirement\">\r\n        <slot name=\"dialog\"> </slot>\r\n      </div>\r\n      <!-- <slot name=\"conserve\"> -->\r\n      <Conserve>\r\n        <el-button\r\n          type=\"primary\"\r\n          icon=\"iconfont el-icon-hospital-passwordbaocun\"\r\n          class=\"keep\"\r\n          slot=\"keep\"\r\n          @click=\"keepform()\"\r\n          >提交</el-button\r\n        >\r\n        <el-button\r\n          type=\"primary\"\r\n          icon=\"iconfont el-icon-hospital-passwordai207\"\r\n          class=\"return\"\r\n          style=\"border: 1px solid #949aef\"\r\n          slot=\"back\"\r\n          @click=\"backss()\"\r\n          >返回</el-button\r\n        >\r\n      </Conserve>\r\n      <!-- </slot> -->\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport Conserve from \"../components/conserve\";\r\n// 获取列表\r\nimport service from \"@/service/index\";\r\nimport qss from \"@/service/qs\";\r\nimport qs from \"qs\";\r\nexport default {\r\n  components: {\r\n    Conserve,\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      \r\n      props: { multiple: true },\r\n      agree: \"\", //同意要求\r\n\r\n      data: [\r\n        {\r\n          name: 1,\r\n        },\r\n      ],\r\n      agentname: \"\", //经办人姓名\r\n      newagentname: \"\", //新经办人姓名\r\n      agentphone: \"\", //经办人手机号\r\n      comnumber: \"\", //业务编号\r\n      comdata: \"\", //投诉日期\r\n      incidentdata: \"\", //事发日期\r\n      comname: \"\", //投诉人姓名\r\n      comgender: \"\", //投诉人性别\r\n      comage: \"岁\", //投诉人年龄\r\n      comagenumber: \"\", //投诉人年龄\r\n      comphone: \"\", //投诉人电话\r\n      consulttime: \"\", //协商时间\r\n      address: \"\", //联系地址\r\n      reason: \"\", //投诉原因\r\n      comgenders: [\r\n        //性别\r\n        {\r\n          value: \"1\",\r\n          lable: \"男\",\r\n        },\r\n        {\r\n          value: \"0\",\r\n          lable: \"女\",\r\n        },\r\n        {\r\n          value: \"2\",\r\n          lable: \"未知\",\r\n        },\r\n      ],\r\n      commodes: [], // 投诉方式列表\r\n      commode: \"\", //投诉方式\r\n      comdes: [], //投诉科室列表\r\n      comde: \"\", //投诉科室\r\n      relationlist: [], //投诉人与患者的关系\r\n      relation: \"\", //关系列表\r\n      comtypelist: [], //投诉类别列表\r\n      comtype: \"\", //投诉类别\r\n      natures: [], //事件性质列表\r\n      nature: \"\", //事件性质\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    getCascaderObj() {\r\n      // console.log(this.comde);\r\n    },\r\n    //保存提交事件\r\n    keepform() {\r\n      // this.$router.go(0);\r\n      var stime = new Date(this.comdata);\r\n      var StartTime =\r\n        stime.getFullYear() +\r\n        \"-\" +\r\n        (stime.getMonth() + 1) +\r\n        \"-\" +\r\n        stime.getDate();\r\n      var etime = new Date(this.incidentdata);\r\n      var EndTime =\r\n        etime.getFullYear() +\r\n        \"-\" +\r\n        (etime.getMonth() + 1) +\r\n        \"-\" +\r\n        etime.getDate();\r\n      let comde = this.comde.map((x) => {\r\n        return x[1];\r\n      });\r\n\r\n      let params = {\r\n        D_I_Number: this.comnumber, //业务编号\r\n        Complaint_Type: this.comtype.toString(), //投诉类型\r\n        Complain_Reason: this.reason, //投诉原因\r\n        Incident_Time: EndTime, //事发时间\r\n        D_ID: comde.toString(), //投诉科室\r\n        Complaint_Time: StartTime, //投诉日期\r\n        Complaint_Model: this.commode, //投诉方式\r\n        C_I_Name: this.comname, //投诉人姓名\r\n        C_I_Phone: this.comphone, //投诉人电话\r\n        C_I_Gender: this.comgender, //投诉人性别\r\n        C_I_Age_Type: 0, //岁月天\r\n        C_I_Age: this.comagenumber, //投诉人年龄\r\n        D_M_ID: this.relation, //患者关系\r\n        Reply_Time: this.consulttime, //协商时间\r\n        Dispute_State: this.nature, //事件性质this.nature\r\n        Ag_Name: this.agentname, //经办人姓名\r\n        New_Ag_Name: this.newagentname, //新经办人姓名\r\n        Ag_Phone: this.agentphone, //经办人手机号\r\n        U_ID: 1, //获取当前登录人ID\r\n      };\r\n\r\n      let uid = 1;\r\n      console.log(params);\r\n      service.AddComponent(qs.stringify(params)).then((res) => {\r\n        // console.log(res);\r\n      });\r\n    },\r\n    backss() {\r\n      this.$router.go(0);\r\n    },\r\n\r\n    //表格点击事件\r\n    handleClick(index, rows) {\r\n      // console.log(index, rows);\r\n    },\r\n    // 设置表头颜色\r\n    getRowClass({ rowIndex }) {\r\n      if (rowIndex == 0) {\r\n        return \"background:#c2c5f6;color:#000\";\r\n      } else {\r\n        return \"\";\r\n      }\r\n    },\r\n  },\r\n  created() {\r\n    service\r\n      .AddManaged(5)\r\n      .then((res) => {\r\n        // console.log(res);\r\n        this.commodes = res.data; // 投诉方式\r\n      })\r\n      .catch((err) => {});\r\n    service.AddDepartment().then((res) => {\r\n      // console.log(res);\r\n      this.comdes = res.data; //投诉科室\r\n    });\r\n    service.AddCom().then((res) => {\r\n      // console.log(res);\r\n      this.comnumber = res.number; // 事件编号\r\n    });\r\n    service.AddManaged(1).then((res) => {\r\n      // console.log(res);\r\n      this.relationlist = res.data; // 关系列表\r\n    });\r\n    service.AddManaged(3).then((res) => {\r\n      // console.log(res);\r\n      this.comtypelist = res.data; // 投诉类别\r\n    });\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n@import \"../css/addcom.css\";\r\n</style>\r\n"],"sourceRoot":"src/views/H-Complaints/components"}]}
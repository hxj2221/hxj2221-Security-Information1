        :inactive-value=\"2\"\r\n              active-color=\"#02538C\"\r\n              inactive-color=\"#B9B9B9\"\r\n              @change=\"changeSwitch($event, scope.row)\"\r\n            ></el-switch>\r\n          </template>\r\n        </el-table-column>\r\n\r\n        <el-table-column label=\"操作\" width=\"120\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button\r\n              class=\"staffFotedit\"\r\n              size=\"mini\"\r\n              @click=\"handleEdit(scope.$index, scope.row)\"\r\n              >编辑</el-button\r\n            >\r\n            <el-button\r\n              class=\"staffFotdel\"\r\n              size=\"mini\"\r\n              type=\"danger\"\r\n              @click=\"handleDelete(scope.$index, tables)\"\r\n              >删除</el-button\r\n            >\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <el-pagination\r\n        layout=\"total, prev, pager, next, jumper\"\r\n        :total=\"tables.length\"\r\n      >\r\n      </el-pagination>\r\n    </div>\r\n    <!--新增-->\r\n    <Staff v-show=\"add\"></Staff>\r\n    <!-- 编辑 -->\r\n    <el-dialog\r\n      title=\"编辑\"\r\n      :visible.sync=\"editFormVisible\"\r\n      :close-on-click-modal=\"true\"\r\n      :append-to-body=\"true\"\r\n    >\r\n      <!--editForm表单提交的数据-->\r\n      <el-form :model=\"editForm\" label-width=\"80px\" ref=\"editForm\">\r\n        <el-form-item prop=\"job_number\" label=\"工号\" width=\"100\">\r\n          <el-input\r\n            v-model=\"editForm.job_number\"\r\n            auto-complete=\"off\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"name\" label=\"员工姓名\" width=\"120\">\r\n          <el-input v-model=\"editForm.name\" auto-complete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"sex\" label=\"员工性别\" width=\"100\">\r\n          <el-input v-model=\"editForm.sex\" auto-complete=\"off\"></el-input>\r\n        </el-form-item>\r\n
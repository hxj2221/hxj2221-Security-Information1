w));\r\n\r\n      let params = {\r\n        id: this.id,\r\n      };\r\n      service.staffedits(params).then((res) => {\r\n        console.log(this.id);\r\n        console.log(res);\r\n      });\r\n    },\r\n    //删除：\r\n    handleDelete(index, row) {\r\n      console.log(index, row);\r\n      let params = {\r\n        id: this.id,\r\n      };\r\n      service.staffDel(params).then((res) => {\r\n        console.log(res);\r\n        \r\n      });\r\n      // this.$confirm(\"此操作将永久删除该文件, 是否继续?\", \"提示\", {\r\n      //   confirmButtonText: \"确定\",\r\n      //   cancelButtonText: \"取消\",\r\n      //   type: \"warning\",\r\n      // })\r\n      //   .then(() => {\r\n      //     this.$message({\r\n      //       type: \"success\",\r\n      //       message: \"删除成功!\",\r\n      //       delete: row.splice(index, 1),\r\n      //     });\r\n      //   })\r\n      //   .catch(() => {\r\n      //     this.$message({\r\n      //       type: \"info\",\r\n      //       message: \"已取消删除\",\r\n      //     });\r\n      //   });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n@import \"staff.css\";\r\n</style>\r\n"]}]}
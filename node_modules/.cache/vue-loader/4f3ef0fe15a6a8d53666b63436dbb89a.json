e;\r\n    },\r\n    // 解绑微信\r\n    wxno() {\r\n      this.wxbtn = false;\r\n    },\r\n    // 解绑\r\n    wxyes() {\r\n      this.wxbtn = false;\r\n      this.$confirm(\"你确定要解绑微信吗？\", \"解绑微信\", {\r\n        confirmButtonText: \"确认\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\",\r\n      })\r\n        .then(() => {\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"解绑成功!\",\r\n          });\r\n        })\r\n        .catch(() => {\r\n          this.$message({\r\n            type: \"info\",\r\n            message: \"已取消解绑\",\r\n          });\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style >\r\n@import \"account.css\";\r\n</style>\r\n"]}]}
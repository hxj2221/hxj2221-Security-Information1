          type=\"primary\"\r\n          icon=\"el-icon-circle-plus\"\r\n          @click=\"dialogVisible = true\"\r\n          >新增记录</el-button\r\n        >\r\n        <el-button @click=\"articleList\">返回文章列表</el-button>\r\n      </div>\r\n    </div>\r\n    <hr />\r\n    <!-- 弹框 -->\r\n    <el-dialog\r\n      title=\"添加分类\"\r\n      :visible.sync=\"dialogVisible\"\r\n      width=\"30%\"\r\n      :before-close=\"handleClose\"\r\n    >\r\n      <!-- <span>这是一段信息</span> -->\r\n      <div class=\"dialog_div\">\r\n        <div class=\"dialog_div_select\">\r\n          <span>上级分类:</span>\r\n          <el-select v-model=\"value\" placeholder=\"请选择\">\r\n            <el-option\r\n              v-for=\"item in options\"\r\n              :key=\"item.value\"\r\n              :label=\"item.label\"\r\n              :value=\"item.value\"\r\n            >\r\n            </el-option>\r\n          </el-select>\r\n          <span>一级分类选择\"无\"</span>\r\n        </div>\r\n        <div class=\"dialog_div_title\">\r\n          <span>分类标题:</span>\r\n          <el-input v-model=\"input\" placeholder=\"请输入分类标题\"></el-input>\r\n        </div>\r\n        <div class=\"dialog_div_z\">\r\n          <span>分类权重:</span>\r\n          <el-input v-model=\"input1\" placeholder=\"默认0，越大越靠前\"></el-input>\r\n        </div>\r\n      </div>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"dialogVisible = false\"\r\n          >确 定</el-button\r\n        >\r\n      </span>\r\n    </el-dialog>\r\n    <!-- table -->\r\n    <div class=\"columnList_table\">\r\n      <el-table :data=\"tableData\" border style=\"width: 100%\">\r\n        <el-table-column prop=\"weight\" label=\"权重\"> </el-table-column>\r\n        <el-table-column prop=\"classification\" label=\"分类名称\">\r\n        </el-table-column>\r\n        <el-table-column prop=\"num\" label=\"文章数量\"> </el-table-column>\r\n        <el-table-column prop=\"date\" label=\"更新时间\"> </el-table-column>\r\n        <el-table-column label=\"状态\">\r\n          <el-switch\r\n            v-model=\"tableData.value\"\r\n            active-color=\"#13ce66\"\r\n            inactive-color=\"#ff4949\"\r\n          >\r\n          </el-switch>\r\n        </el-table-column>\r\n        <el-table-column prop=\"name\" label=\"修改人员\"> </el-table-column>\r\n        <el-table-column label=\"操作\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button type=\"text\" size=\"small\" @click=\"dialogVisible = true\"\r\n              >编辑</el-button\r\n            >\r\n            <el-button\r\n              type=\"text\"\r\n              size=\"small\"\r\n              @click=\"handDel(scope.$index, tableData)\"\r\n              >删除</el-button\r\n            >\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <!-- 分页 -->\r\n      <div class=\"columnList_table_paging\">\r\n        <el-pagination\r\n          :current-page.sync=\"currentPage1\"\r\n          :page-size=\"100\"\r\n          layout=\"total, prev, pager, next\"\r\n          :total=\"tableData.length\"\r\n        >\r\n        </el-pagination>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// import AdminHead from './components/AdminHead'\r\nimport \"@/views/H-Columnlist/css.css\";\r\nexport default {\r\n  components: {},\r\n\r\n  data() {\r\n    return {\r\n      tableData: [\r\n        {\r\n          date: \"2016-05-02\",\r\n          name: \"王小虎\",\r\n          weight: \"上海\",\r\n          classification: \"普陀区\",\r\n          num: \"12\",\r\n          value: false,\r\n        },\r\n        {\r\n          date: \"2016-05-02\",\r\n          name: \"王小虎\",\r\n          weight: \"上海\",\r\n          classification: \"普陀区\",\r\n          num: \"12\",\r\n          value: false,\r\n        },\r\n        {\r\n          date: \"2016-05-02\",\r\n          name: \"王小虎\",\r\n          weight: \"上海\",\r\n          classification: \"普陀区\",\r\n          num: \"12\",\r\n          value: true,\r\n        },\r\n      ],\r\n      dialogVisible: false,\r\n      //\r\n      options: [\r\n        {\r\n          value: \"选项1\",\r\n          label: \"黄金糕\",\r\n        },\r\n        {\r\n          value: \"选项2\",\r\n          label: \"双皮奶\",\r\n        },\r\n        {\r\n          value: \"选项3\",\r\n          label: \"蚵仔煎\",\r\n        },\r\n        {\r\n          value: \"选项4\",\r\n          label: \"龙须面\",\r\n        },\r\n        {\r\n          value: \"选项5\",\r\n          label: \"北京烤鸭\",\r\n        },\r\n      ],\r\n      value: \"\",\r\n      input: \"\",\r\n      input1: \"\",\r\n      currentPage1:1\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    articleList() {\r\n      this.$router.push(\"/Articlelist\");\r\n    },\r\n    handleClose(done) {\r\n      this.$confirm(\"确认关闭？\")\r\n        .then((_) => {\r\n          done();\r\n        })\r\n        .catch((_) => {});\r\n    },\r\n    handDel(i, row) {\r\n      row.splice(i, 1);\r\n       this.$message({\r\n          showClose: true,\r\n          message: '删除成功',\r\n          type: 'success'\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped></style>\r\n"]}]}
>\r\n        <el-table-column type=\"index\" width=\"50\" label=\"序号\"> </el-table-column>\r\n        <el-table-column prop=\"event_number\" label=\"事件编码\"></el-table-column>\r\n        <el-table-column prop=\"complaint_name\" label=\"投诉人姓名\"> </el-table-column>\r\n        <el-table-column prop=\"sex\" label=\"性别\"> </el-table-column>\r\n        <el-table-column prop=\"age\" label=\"年龄\"> </el-table-column>\r\n        <el-table-column prop=\"complaint_phone\" label=\"手机号码\"> </el-table-column>\r\n        <el-table-column prop=\"department.title\" label=\"投诉科室\"> </el-table-column>\r\n        <el-table-column prop=\"complaint_type.title\" label=\"投诉方式\"> </el-table-column>\r\n        <el-table-column prop=\"create_time\" label=\"投诉时间\"> </el-table-column>\r\n        <el-table-column prop=\"pass_department\" label=\"流转部门\"> </el-table-column>\r\n        <el-table-column prop=\"state\" label=\"事件状态\"> </el-table-column>\r\n        <slot name=\"column\">\r\n          <el-table-column fixed=\"right\" label=\"操作\" width=\"150%\">\r\n            <template slot-scope=\"scope\">\r\n              <el-button type=\"text\" size=\"small\">查看</el-button>\r\n              <el-button type=\"text\" size=\"small\">医患记录</el-button>\r\n              <el-button type=\"text\" size=\"small\">操作</el-button>\r\n            </template>\r\n          </el-table-column>\r\n        </slot>\r\n      </el-table>\r\n    </div>\r\n     <div class=\"Complaints-footer\">\r\n        <el-pagination\r\n      @size-change=\"handleSizeChange\"\r\n      @current-change=\"handleCurrentChange\"\r\n      :current-page=\"Page\"\r\n      :page-sizes=\"numberlist\"\r\n      :page-size=\"100\"\r\n      layout=\"total, sizes, prev, pager, next, jumper\"\r\n      :total=\"total\">\r\n    </el-pagination>\r\n      </div>\r\n  </div>\r\n \r\n</template>\r\n<script>\r\nimport service from \"@/service/index\";\r\n\r\nexport default {\r\n  components: {},\r\n\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      numberlist:[5, 8, 10, 20],\r\n     Page:1,\r\n      total:100,\r\n      number:8\r\n    };\r\n  },\r\n\r\n  methods: {\r\n     handleSizeChange(val) {\r\n        console.log(`每页 ${val} 条`);\r\n        this.number=val\r\n         service.ComList(this.number, this.Page).then((res) => {\r\n      if (res.code === 20010) {\r\n        this.tableData = res.data[0];\r\n        this.total=res.data[1].count\r\n        this.Page=res.data[1].current\r\n      } else {\r\n        this.$message({\r\n          message: res.msg,\r\n          type: \"error\",\r\n          duration: 1000,\r\n        });\r\n        // this.$router.push(\"/login\");\r\n      }\r\n    });\r\n      },\r\n      handleCurrentChange(val) {\r\n        this.Page=val\r\n         service.ComList(this.number, this.Page).then((res) => {\r\n      console.log(res);\r\n      if (res.code === 20010) {\r\n        this.tableData = res.data[0];\r\n        this.total=res.data[1].count\r\n        this.Page=res.data[1].current\r\n      } else {\r\n        this.$message({\r\n          message: res.msg,\r\n          type: \"error\",\r\n          duration: 1000,\r\n        });\r\n        // this.$router.push(\"/login\");\r\n      }\r\n    });\r\n      },\r\n    handle(row) {\r\n      console.log(row);\r\n    },\r\n    // 设置表头颜色\r\n    getRowClass({ rowIndex }) {\r\n      if (rowIndex == 0) {\r\n        return \"background:#c2c5f6;color:#000\";\r\n      } else {\r\n        return \"\";\r\n      }\r\n    },\r\n  },\r\n  created() {\r\n    service.ComList(this.number,this.Page).then((res) => {\r\n      console.log(res);\r\n      if (res.code === 20010) {\r\n        this.tableData = res.data[0];\r\n        this.total=res.data[1].count\r\n        this.Page=res.data[1].current\r\n      } else {\r\n        this.$message({\r\n          message: res.msg,\r\n          type: \"error\",\r\n          duration: 1000,\r\n        });\r\n        // this.$router.push(\"/login\");\r\n      }\r\n    });\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n@import \"../css/Table.css\";\r\n</style>\r\n"]}]}
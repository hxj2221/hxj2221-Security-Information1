"space-around\">\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple\">\r\n              <span> 业务编号 </span>\r\n              <el-input v-model=\"number\" disabled></el-input>\r\n            </div>\r\n          </el-col>\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple-light\">\r\n              <span> 投诉日期<span>*</span> </span> <br />\r\n              <el-date-picker\r\n                v-model=\"comdata\"\r\n                type=\"date\"\r\n                placeholder=\"选择日期\"\r\n              ></el-date-picker></div\r\n          ></el-col>\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple-light\">\r\n              <span> 事发日期<span>*</span> </span> <br />\r\n              <el-date-picker v-model=\"comdata\" type=\"date\" placeholder=\"选择日期\">\r\n              </el-date-picker></div\r\n          ></el-col>\r\n        </el-row>\r\n        <el-row type=\"flex\" class=\"row-bg\" justify=\"space-around\">\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple\">\r\n              <span> 投诉人姓名 </span>\r\n              <el-input v-model=\"comname\" placeholder=\"请输入投诉人姓名\"></el-input>\r\n            </div>\r\n          </el-col>\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple-light\">\r\n              <span> 性别<span>*</span> </span> <br />\r\n              <el-select v-model=\"comgender\" placeholder=\"请选择\">\r\n                <el-option\r\n                  v-for=\"item in comgenders\"\r\n                  :key=\"item.value\"\r\n                  :label=\"item.label\"\r\n                  :value=\"item.value\"\r\n                >\r\n                </el-option>\r\n              </el-select></div\r\n          ></el-col>\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple-light\">\r\n              <span> 年龄</span> <br />\r\n              <el-input\r\n                placeholder=\"请输入内容\"\r\n                v-model=\"comagenumber\"\r\n                class=\"input-with-select\"\r\n              >\r\n                <template slot=\"append\">\r\n                  <el-select v-model=\"comage\" slot=\"prepend\" style=\"width: 80px\">\r\n                    <el-option label=\"岁\" value=\"1\"></el-option>\r\n                    <el-option label=\"月\" value=\"2\"></el-option>\r\n                    <el-option label=\"日\" value=\"3\"></el-option> </el-select\r\n                ></template>\r\n              </el-input></div\r\n          ></el-col>\r\n        </el-row>\r\n        <el-row type=\"flex\" class=\"row-bg\" justify=\"space-around\">\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple\">\r\n              <span> 手机号码 </span>\r\n              <el-input v-model=\"comphone\" placeholder=\"请输入投诉人手机号码\"></el-input>\r\n            </div>\r\n          </el-col>\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple-light\">\r\n              <span> 投诉方式<span>*</span> </span> <br />\r\n              <el-select v-model=\"commode\" placeholder=\"请选择\">\r\n                <el-option\r\n                  v-for=\"item in commodes\"\r\n                  :key=\"item.value\"\r\n                  :label=\"item.label\"\r\n                  :value=\"item.value\"\r\n                >\r\n                </el-option>\r\n              </el-select></div\r\n          ></el-col>\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple-light\">\r\n              <span> 投诉科室<span>*</span> </span> <br />\r\n              <el-select v-model=\"comde\" placeholder=\"请选择\">\r\n                <el-option\r\n                  v-for=\"item in comdes\"\r\n                  :key=\"item.value\"\r\n                  :label=\"item.label\"\r\n                  :value=\"item.value\"\r\n                >\r\n                </el-option>\r\n              </el-select></div\r\n          ></el-col>\r\n        </el-row>\r\n        <el-row type=\"flex\" class=\"row-bg\" justify=\"space-around\">\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple\">\r\n              <span> 投诉人与患者关系 </span><br />\r\n              <el-select v-model=\"relation\" placeholder=\"请选择\">\r\n                <el-option\r\n                  v-for=\"item in relationlist\"\r\n                  :key=\"item.value\"\r\n                  :label=\"item.label\"\r\n                  :value=\"item.value\"\r\n                >\r\n                </el-option>\r\n              </el-select>\r\n            </div>\r\n          </el-col>\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple-light\">\r\n              <span> 答复协商时间</span> <br />\r\n              <el-input placeholder=\"请输入内容\" v-model=\"consulttime\" style=\"width: 60%\">\r\n                <template slot=\"append\">天</template>\r\n              </el-input>\r\n            </div></el-col\r\n          >\r\n          <el-col :span=\"6\"\r\n            ><div class=\"grid-content bg-purple-light\">\r\n              <span> 投诉类别(多选)</span> <br />\r\n              <el-select v-model=\"comtype\" multiple placeholder=\"请选择\">\r\n                <el-option\r\n                  v-for=\"item in comtypelist\"\r\n                  :key=\"item.value\"\r\n                  :label=\"item.label\"\r\n                  :value=\"item.value\"\r\n                >\r\n                </el-option>\r\n              </el-select></div\r\n          ></el-col>\r\n        </el-row>\r\n        <el-row type=\"flex\" class=\"row-bg\">\r\n          <el-col :span=\"6\" :push=\"1\"\r\n            ><div class=\"grid-content bg-purple\">\r\n              <span> 事件性质 </span><br />\r\n              <el-select v-model=\"nature\" placeholder=\"请选择\">\r\n                <el-option\r\n                  v-for=\"item in natures\"\r\n                  :key=\"item.value\"\r\n                  :label=\"item.label\"\r\n                  :value=\"item.value\"\r\n                >\r\n                </el-option>\r\n              </el-select>\r\n            </div>\r\n          </el-col>\r\n          <el-col :span=\"12\" :push=\"3\"\r\n            ><div class=\"grid-content bg-purple-light\">\r\n              <span> 联系地址<span>*</span> </span> <br />\r\n              <el-input v-model=\"address\" placeholder=\"联系地址\"></el-input></div\r\n          ></el-col>\r\n        </el-row>\r\n        <el-row type=\"flex\" class=\"row-bg\">\r\n          <el-col :span=\"20\" :push=\"1\"\r\n            ><div class=\"grid-content bg-purple\">\r\n              <span> 投诉原因</span\r\n              ><el-input\r\n                type=\"textarea\"\r\n                v-model=\"reason\"\r\n                placeholder=\"请输入投诉原因\"\r\n              ></el-input>\r\n            </div>\r\n          </el-col>\r\n        </el-row>\r\n      </div>\r\n      <!-- 经办人 -->\r\n      <div class=\"Addcom-agent\">\r\n        <h4>经办人信息</h4>\r\n        <div>\r\n          <el-row type=\"flex\" class=\"row-bg\" justify=\"space-around\">\r\n            <el-col :span=\"6\" :pull=\"2\"\r\n              ><div class=\"grid-content bg-purple\">\r\n                <span> 经办人姓名<span>*</span> </span>\r\n                <el-input v-model=\"agentname\" placeholder=\"请输入姓名\"></el-input>\r\n              </div>\r\n            </el-col>\r\n            <el-col :span=\"6\" :pull=\"6\"\r\n              ><div class=\"grid-content bg-purple-light\">\r\n                <span>经办人手机<span>*</span> </span> <br />\r\n                <el-input\r\n                  v-model=\"agentphone\"\r\n                  placeholder=\"请输入手机号码\"\r\n                ></el-input></div\r\n            ></el-col>\r\n          </el-row>\r\n        </div>\r\n      </div>\r\n      <!-- 患者信息 -->\r\n      <div class=\"addcom-relevance\">\r\n        <h4>关联患者信息(调用HIS)</h4>\r\n        <el-table\r\n          :data=\"data\"\r\n          style=\"width: 94%; margin-left: 3%; margin-top: 20px; margin-bottom: 20px;\"\r\n          :header-cell-style=\"getRowClass\"\r\n        >\r\n          <el-table-column prop=\"name\" label=\"序号\"> </el-table-column>\r\n          <el-table-column prop=\"name\" label=\"序号\"> </el-table-column>\r\n          <el-table-column prop=\"name\" label=\"序号\"> </el-table-column>\r\n          <el-table-column prop=\"name\" label=\"序号\"> </el-table-column>\r\n          <el-table-column prop=\"name\" label=\"序号\"> </el-table-column>\r\n          <el-table-column prop=\"name\" label=\"序号\"> </el-table-column>\r\n          <el-table-column prop=\"name\" label=\"序号\"> </el-table-column>\r\n          <el-table-column fixed=\"right\" label=\"操作\" width=\"100%\" style=\"border-bottom:none\">\r\n            <template >\r\n              <el-button type=\"text\" size=\"small\" @click=\"handleClick() \" style=\"border-bottom:none\"\r\n                >取消关联</el-button\r\n              >\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n        <div align=\"center\">\r\n          <el-button type=\"primary\" icon=\"el-icon-circle-plus\" class=\"addrelevance\"\r\n            >添加患者信息</el-button\r\n          >\r\n        </div>\r\n      </div>\r\n      <!-- 弹窗 -->\r\n      <div class=\"requirement\">\r\n        <slot name=\"dialog\">\r\n        </slot>\r\n      </div>\r\n      <slot name=\"conserve\"> </slot>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport Conserve from \"../components/conserve\";\r\nexport default {\r\n  components: {\r\n    Conserve,\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      agree: \"\", //同意要求\r\n    \r\n      data: [\r\n        {\r\n          name: 1,\r\n        },\r\n      ],\r\n      agentname: \"\", //经办人姓名\r\n      agentphone: \"\", //经办人手机号\r\n      number: 776567788, //业务编号\r\n      comdata: \"\", //投诉日期\r\n      incidentdata: \"\", //事发日期\r\n      comname: \"\", //投诉人姓名\r\n      comgender: \"\", //投诉人性别\r\n      comage: \"岁\", //投诉人年龄\r\n      comagenumber: \"\",\r\n      comphone: \"\", //投诉人电话\r\n      consulttime: \"\", //协商时间\r\n      address: \"\", //联系地址\r\n      reason: \"\", //投诉原因\r\n      comgenders: [\r\n        {\r\n          value: \"男\",\r\n          lable: \"男\",\r\n        },\r\n        {\r\n          value: \"女\",\r\n          lable: \"女\",\r\n        },\r\n      ],\r\n      commodes: [\r\n        {\r\n          value: \"男\",\r\n          lable: \"男\",\r\n        },\r\n        {\r\n          value: \"女\",\r\n          lable: \"女\",\r\n        },\r\n      ],\r\n      commode: \"\", //投诉方式\r\n      comdes: [\r\n        {\r\n          value: \"男\",\r\n          lable: \"男\",\r\n        },\r\n        {\r\n          value: \"女\",\r\n          lable: \"女\",\r\n        },\r\n      ],\r\n      comde: \"\",\r\n      relationlist: [\r\n        {\r\n          value: \"本人\",\r\n          lable: \"本人\",\r\n        },\r\n        {\r\n          value: \"亲属\",\r\n          lable: \"亲属\",\r\n        },\r\n      ],\r\n      relation: \"\", //关系\r\n      comtypelist: [\r\n        {\r\n          value: \"服务态度\",\r\n          lable: \"服务态度\",\r\n        },\r\n        {\r\n          value: \"费用\",\r\n          lable: \"费用\",\r\n        },\r\n      ],\r\n      comtype: \"\", //投诉类别\r\n      natures: [\r\n        {\r\n          value: \"投诉\",\r\n          lable: \"投诉\",\r\n        },\r\n        {\r\n          value: \"纠纷\",\r\n          lable: \"投诉\",\r\n        },\r\n      ], //事件性质\r\n      nature: \"\",\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    //保存提交事件\r\n    keepform() {\r\n      this.$router.go(0);\r\n    },\r\n    back() {\r\n      this.$router.go(0);\r\n    },\r\n   \r\n    //表格点击事件\r\n    handleClick() {},\r\n    // 设置表头颜色\r\n    getRowClass({ rowIndex }) {\r\n      if (rowIndex == 0) {\r\n        return \"background:#c2c5f6\";\r\n      } else {\r\n        return \"\";\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n@import \"../css/addcom.css\";\r\n</style>\r\n"]}]}